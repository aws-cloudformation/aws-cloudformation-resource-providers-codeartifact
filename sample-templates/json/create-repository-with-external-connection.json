{
  "Parameters": {
    "DomainName": {
      "Type": "String"
    },
    "RepoName": {
      "Type": "String"
    },
    "ExternalConnection": {
      "Type": "String"
    }
  },
  "Resources": {
    "CodeArtifactRepository": {
      "Type": "AWSdevTools::CodeArtifact::Repository",
      "Properties": {
        "RepositoryName": { "Ref" : "RepoName" },
        "DomainName": { "Ref" : "DomainName" },
        "ExternalConnection": [
          { "Ref" : "ExternalConnection" }
        ]
      }
    }
  }
}
