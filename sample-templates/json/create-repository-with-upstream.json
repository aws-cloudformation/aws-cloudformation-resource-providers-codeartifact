{
  "Parameters": {
    "DomainName": {
      "Type": "String"
    },
    "RepoName": {
      "Type": "String"
    },
    "UpstreamRepoName": {
      "Type": "String"
    }
  },
  "Resources": {
    "CodeArtifactUpstreamRepository": {
      "Type": "AWSdevTools::CodeArtifact::Repository",
      "Properties": {
        "RepositoryName": { "Ref" : "UpstreamRepoName" },
        "DomainName": { "Ref" : "DomainName" }
      }
    },
    "CodeArtifactRepository": {
      "DependsOn": "CodeArtifactUpstreamRepository",
      "Type": "AWSdevTools::CodeArtifact::Repository",
      "Properties": {
        "RepositoryName": { "Ref" : "RepoName" },
        "DomainName":{ "Ref" : "DomainName" },
        "Upstreams": [
          { "Ref" : "UpstreamRepoName" }
        ]
      }
    }
  }
}
